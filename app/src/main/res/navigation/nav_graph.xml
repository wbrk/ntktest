<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/feedFragment">

    <fragment
        android:id="@+id/feedFragment"
        android:name="ntktest.example.com.ntktest.presentation.feed.FeedFragment"
        android:label="@string/feed_screen"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_sourceFragment"
            app:destination="@id/sourceFragment" />
    </fragment>

    <fragment
        android:id="@+id/sourceFragment"
        android:name="ntktest.example.com.ntktest.presentation.source.SourceFragment"
        android:label="@string/sources_screen"
        tools:layout="@layout/fragment_source" />
</navigation>
